<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
 <managed-bean>
  <managed-bean-name>pruebaComponent</managed-bean-name>
  <managed-bean-class>com.fabiandeita.controlescolar.bean.PruebaControlador</managed-bean-class>
 
  <managed-bean-scope>session</managed-bean-scope></managed-bean>
 
  <navigation-rule>
      <from-view-id>*</from-view-id>
      <navigation-case>
          <from-outcome>jobApplicantPage</from-outcome>
          <to-view-id>/job-applicant.xhtml</to-view-id>
      </navigation-case>
  </navigation-rule>
  
  <navigation-rule>
      <from-view-id>*</from-view-id>
      <navigation-case>
          <from-outcome>cancelJobApplicant</from-outcome>
          <to-view-id>/applicants.xhtml</to-view-id>
          <redirect />
      </navigation-case>
  </navigation-rule>
 
</faces-config>
